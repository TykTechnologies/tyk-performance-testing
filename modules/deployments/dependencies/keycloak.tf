resource "kubernetes_deployment" "keycloak_operator_deployment" {
  metadata {
    name      = "keycloak-operator"
    namespace = var.namespace

    annotations = {
      "app.quarkus.io/quarkus-version"  = "3.8.5"
      "app.quarkus.io/vcs-uri"          = "https://github.com/stianst/keycloak.git"
      "app.quarkus.io/build-timestamp"  = "2024-07-18 - 07:20:41 +0000"
    }

    labels = {
      "app.kubernetes.io/name"       = "keycloak-operator"
      "app.kubernetes.io/version"    = "25.0.2"
      "app.kubernetes.io/managed-by" = "quarkus"
    }
  }

  spec {
    replicas = 1

    selector {
      match_labels = {
        "app.kubernetes.io/name" = "keycloak-operator"
      }
    }

    template {
      metadata {
        annotations = {
          "app.quarkus.io/quarkus-version"  = "3.8.5"
          "app.quarkus.io/vcs-uri"          = "https://github.com/stianst/keycloak.git"
          "app.quarkus.io/build-timestamp"  = "2024-07-18 - 07:20:41 +0000"
        }

        labels = {
          "app.kubernetes.io/managed-by" = "quarkus"
          "app.kubernetes.io/name"       = "keycloak-operator"
          "app.kubernetes.io/version"    = "25.0.2"
        }
      }

      spec {
        container {
          name  = "keycloak-operator"
          image = "quay.io/keycloak/keycloak-operator:25.0.2"
          image_pull_policy = "Always"

          env {
            name = "KUBERNETES_NAMESPACE"
            value_from {
              field_ref {
                field_path = "metadata.namespace"
              }
            }
          }

          env {
            name  = "RELATED_IMAGE_KEYCLOAK"
            value = "quay.io/keycloak/keycloak:25.0.2"
          }

          port {
            container_port = 8080
            name           = "http"
            protocol       = "TCP"
          }

          liveness_probe {
            http_get {
              path   = "/q/health/live"
              port   = 8080
              scheme = "HTTP"
            }
            initial_delay_seconds = 5
            period_seconds        = 10
            timeout_seconds       = 10
            success_threshold     = 1
            failure_threshold     = 3
          }

          readiness_probe {
            http_get {
              path   = "/q/health/ready"
              port   = 8080
              scheme = "HTTP"
            }
            initial_delay_seconds = 5
            period_seconds        = 10
            timeout_seconds       = 10
            success_threshold     = 1
            failure_threshold     = 3
          }

          startup_probe {
            http_get {
              path   = "/q/health/started"
              port   = 8080
              scheme = "HTTP"
            }
            initial_delay_seconds = 5
            period_seconds        = 10
            timeout_seconds       = 10
            success_threshold     = 1
            failure_threshold     = 3
          }
        }

        service_account_name = "keycloak-operator"
      }
    }
  }

  count      = var.keycloak.enabled ? 1 : 0
  depends_on = [kubernetes_namespace.dependencies]
}

resource "kubernetes_secret" "keycloak-initial-admin" {
  metadata {
    name      = "keycloak-initial-admin"
    namespace = var.namespace
  }
  data = {
    username = "admin"
    password = "topsecretpassword"
  }

  count      = var.keycloak.enabled ? 1 : 0
  depends_on = [kubernetes_namespace.dependencies]
}

